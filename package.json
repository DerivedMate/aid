{
  "name": "reactjs-typescript-pwa",
  "version": "1.0.0",
  "description": "A fast and secure Progressive Web App built with ReactJS with every best practices for SEO and web performances. Full TypeScript !",
  "main": "src/index.tsx",
  "scripts": {
    "start": "npm run start:prod",
    "start:dev": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=\"development\" webpack-dev-server --color --watch --hot --stdin --config config/webpack/app.dev.ts",
    "start:test": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=\"test\" webpack-dev-server --color --watch --hot --stdin --config config/webpack/app.dev.ts",
    "start:prod": "cross-env NODE_ENV=\"production\" ts-node -P \"./tsconfig.json\" --files server/index.ts",
    "server:dev": "DATABASE_URL=\"postgresql://aidclient:toor@localhost:5432/aid\" NODE_ENV=\"development\" nodemon --watch server --exec \"kill -9 `lsof -t -i:3001`; ts-node -P \"./tsconfig.json\" --files \" ./server/index.ts",
    "server:test": "NODE_ENV=\"test\" nodemon --watch server --exec \"kill -9 `lsof -t -i:3001`; ts-node -P \"./tsconfig.json\" --files \" ./server/index.ts",
    "build": "cross-env DS_PYTHON_VERSION=2 TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=\"production\" webpack --progress --fix --colors --config config/webpack/app.prod.ts",
    "postbuild": "npm run build:sw:prod",
    "build:sw:prod": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=\"production\" webpack --progress --colors --config config/webpack/service-worker.prod.ts && ts-node config/service-worker/inject-manifest.ts",
    "build:local": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=\"production\" webpack --progress --colors --config config/webpack/app.prod.ts",
    "postbuild:local": "npm run build:sw:local",
    "build:sw:local": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=\"development\" webpack --progress --colors --config config/webpack/service-worker.dev.ts && ts-node config/service-worker/inject-manifest.ts",
    "test": "jest --colors --config config/tests/jest.config.js",
    "test:watch": "jest --collectCoverage=false --colors --watch --config config/tests/jest.config.js",
    "ts:check": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "lint": "npm run eslint && npm run stylelint && npm run markdownlint && npm run prettier && npm run htmlhint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run markdownlint:fix && npm run prettier:fix",
    "eslint": "eslint --fix --color --ext .jsx --ext .js --ext .ts --ext .tsx src/",
    "eslint:fix": "eslint --fix --color --ext .jsx --ext .js --ext .ts --ext .tsx src/",
    "stylelint": "stylelint \"**/*.{css,scss,less,sass,js,jsx,ts,tsx,html}\"",
    "stylelint:fix": "stylelint --fix \"**/*.{css,scss,less,sass,js,jsx,ts,tsx,html}\"",
    "markdownlint": "markdownlint \"**/*.md\"",
    "markdownlint:fix": "markdownlint --fix \"**/*.md\"",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "htmlhint": "htmlhint --config .htmlhintrc --ignore node_modules/ **/*.ejs **/*.html",
    "browsers": "browserslist",
    "browsers:coverage": "browserslist --coverage",
    "browsers:autoprefixer": "autoprefixer --info",
    "prestats": "mkdir -p stats && cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=\"production\" webpack --profile --json --config config/webpack/app.prod.ts > stats/stats.json",
    "stats": "webpack-bundle-analyzer stats/stats.json build/",
    "clean": "rm -rf stats && rm -rf coverage && rm -rf build"
  },
